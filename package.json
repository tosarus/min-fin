    "@mui/material": "^6.4.8",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.6.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
