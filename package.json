{
  "name": "min-fin",
  "version": "0.2.5",
  "private": true,
  "engines": {
    "node": "<23",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "run-s clean build:*",
    "build:client": "vite build",
    "build:server": "tsc --build server",
    "clean": "rimraf -rf build dist",
    "dev": "run-p dev:server dev:client",
    "dev:client": "vite --port 3000",
    "dev:server": "nodemon server/index.ts 3010",
    "lint": "eslint",
    "migrate": "cross-env TS_NODE_PROJECT=server/tsconfig.json ts-node node_modules/.bin/node-pg-migrate -j ts -m server/database/migrations",
    "start": "node dist/server"
  },
  "proxy": "http://localhost:3010",
  "dependencies": {
    "@fast-csv/parse": "^5.0.2",
    "@tosarus/routing-express": "0.6.0",
    "axios": "^1.8.3",
    "body-parser": "^1.20.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jwks-rsa": "^3.0.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
