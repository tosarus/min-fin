{
  "name": "min-fin",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "<23",
    "yarn": "1.x"
  },
  "scripts": {
    "start": "node dist/server",
    "build": "run-s clean build:*",
    "build:client": "vite build",
    "build:server": "tsc --build server",
    "dev": "run-p server client",
    "client": "vite --port 3000",
    "server": "nodemon server/index.ts 3010",
    "clean": "rimraf -rf build dist",
    "migrate": "cross-env TS_NODE_PROJECT=server/tsconfig.json ts-node node_modules/.bin/node-pg-migrate -j ts -m server/database/migrations",
    "lint": "eslint \"{src,server,shared}/**/*.{ts,tsx}\""
  },
  "proxy": "http://localhost:3010",
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@tosarus/routing-express": "0.5.36",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "helmet": "^6.0.0",
    "jwks-rsa": "^3.0.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
