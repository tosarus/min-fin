{
  "name": "min-fin",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "node dist/server",
    "build": "run-s clean build:*",
    "build:client": "INLINE_RUNTIME_CHUNK=false BUILD_PATH=dist/build react-app-rewired build",
    "build:server": "tsc --build server",
    "dev": "run-p server client",
    "client": "react-app-rewired start",
    "server": "nodemon server/index.ts 3010 500",
    "clean": "rm -rf build dist",
    "eject": "react-scripts eject",
    "migrate": "TS_NODE_PROJECT=server/tsconfig.json ts-node node_modules/.bin/node-pg-migrate -j ts -m server/database/migrations",
    "lint": "eslint --ext .ts,.tsx --color server/ shared/ src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3010",
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@tosarus/routing-express": "0.5.36",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^7.7.7",
    "helmet": "^6.0.0",
    "jwks-rsa": "^3.0.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^1.22.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "clsx": "^1.1.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "formik": "^2.2.9",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-app-rewire-alias": "^1.1.7",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.1",
    "redux-toolkit-saga": "^1.2.0",
    "source-map-explorer": "^2.5.3",
    "ts-node": "^10.9.1",
    "typescript": "4.6.x",
    "wouter": "^2.8.0"
  }
}
