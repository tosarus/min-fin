{
  "name": "min-fin",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "scripts": {
    "start": "node dist",
    "build": "run-p build:client build:server",
    "build:client": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:server": "rm -rf dist && tsc --build tsconfig.server.json",
    "dev": "run-p server client",
    "client": "react-scripts start",
    "server": "nodemon server/index.ts 3010 500",
    "server:self": "nodemon server/index.ts 3000",
    "clean": "rm -rf build dist",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3010",
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "helmet": "^4.6.0",
    "jwks-rsa": "^2.0.4",
    "morgan": "^1.10.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^1.6.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/dateformat": "^3.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^12.0.0",
    "@types/pg": "^8.6.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/wouter": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "clsx": "^1.1.1",
    "dateformat": "^4.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.25.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.2",
    "wouter": "^2.7.4"
  }
}
