{
  "name": "min-fin",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "node dist/server",
    "build": "run-s clean build:*",
    "build:client": "INLINE_RUNTIME_CHUNK=false BUILD_PATH=dist/build react-app-rewired build",
    "build:server": "tsc --build server",
    "build:assets": "cp -R assets dist/",
    "dev": "run-p server client",
    "client": "react-app-rewired start",
    "server": "nodemon server/index.ts 3010 500",
    "clean": "rm -rf build dist",
    "eject": "react-scripts eject",
    "migrate": "TS_NODE_PROJECT=server/tsconfig.json ts-node node_modules/.bin/node-pg-migrate -j ts -m server/database/migrations",
    "lint": "eslint --ext .ts,.tsx --color server/ shared/ src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3010",
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@tosarus/routing-express": "^0.5.6",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.2",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "express-jwt": "^7.7.2",
    "helmet": "^5.1.0",
    "jwks-rsa": "^2.1.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-pg-migrate": "^6.2.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^1.22.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.2",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "clsx": "^1.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.25.1",
    "formik": "^2.2.9",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-app-rewire-alias": "^1.1.5",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "4.0.3",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-toolkit-saga": "^1.2.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^10.8.0",
    "typescript": "4.4.x",
    "wouter": "^2.8.0-alpha.2"
  }
}
