    "@mui/material": "^7.3.5",
    "@reduxjs/toolkit": "^2.10.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.6",
