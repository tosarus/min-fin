    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-saga": "^1.2.1",
    "redux-toolkit-saga": "^1.2.0",
