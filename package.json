    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jwks-rsa": "^3.2.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.16.3",
